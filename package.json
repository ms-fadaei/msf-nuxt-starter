{
  "name": "msf-nuxt-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build --modern=server",
    "build:legacy": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "analyze": "yarn build --analyze",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:style:fix": "yarn lint:style --fix",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:fix": "yarn lint:js:fix && yarn lint:style:fix",
    "test:unit": "jest --runTestsByPath=test/unit/**/*.spec.js",
    "test:unit:watch": "yarn test:unit --watchAll",
    "test:unit:cover": "yarn test:unit --collect-coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/pwa": "3.3.5",
    "core-js": "3.17.3",
    "nuxt": "2.15.8"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/tailwindcss": "6.12.2",
    "@vue/test-utils": "1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "7.17.0",
    "fibers": "5.0.0",
    "husky": "7.0.2",
    "jest": "26.6.3",
    "lint-staged": "11.1.2",
    "postcss": "8.3.6",
    "prettier": "2.4.0",
    "sass": "1.32.13",
    "sass-loader": "10",
    "standard-version": "9.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recess-order": "2.5.0",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.20.1",
    "vue-jest": "3.0.7"
  }
}
